name: cisTEMx c++std=20

on:
  push:
    branches: main
  pull_request:
    branches: main

jobs:
  run_build:  
    uses: ./.github/workflows/run_builds.yml
    with:
      build_type: debug
      configure_options: "--disable-staticmode --enable-openmp --enable-debugmode --with-c++std=20"
      runs_on_os: "ubuntu-20.04"
      CC: gcc
      CXX: g++
      cppstandard: 20

